{
  "name": "webpack-samples",
  "version": "1.0.0",
  "description": "Some examples for webpack configurations",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build:all": "npm run build:base && npm run build:loaders && npm run build:plugins && npm run build:media && npm run build:babel",
    "build:base": "webpack --config base.config.js",
    "build:loaders": "webpack --config loaders.config.js",
    "build:plugins": "webpack --config plugins.config.js",
    "build:media": "webpack --config media.config.js",
    "build:babel": "webpack --config wbabel.config.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
